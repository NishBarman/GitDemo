import io.restassured.RestAssured;
import io.restassured.path.json.JsonPath;

import static io.restassured.RestAssured.*;



public class oAuthTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
	
		String accesstokenresponse=given().urlEncodingEnabled(false).queryParams("code","4%2F4QHOGWLtg3OxFhrfiQX8di32IKl3lvxa8iMvHW0Aq-zkx2mX_PSaW9soV_x8fhqUx3KkdfQ5cqseUXGPtqzB6bs")
		.queryParams("client_id","692183103107-p0m7ent2hk7suguv4vq22hjcfhcr43pj.apps.googleusercontent.com")
		.queryParams("client_secret","erZOWM9g3UtwNRj340YYaK_W")
		.queryParams("redirect_uri","https://rahulshettyacademy.com/getCourse.php")
		.queryParams("grant_type","authorization_code").when().log().all()
		.post("https://www.googleapis.com/oauth2/v4/token").asString();
		
		JsonPath js=new JsonPath(accesstokenresponse);
		String acesstoken=js.getString("access_token");
		
		
		
		String response=given().queryParam("access_token", acesstoken).when().get("https://rahulshettyacademy.com/getCourse.php").asString();
		System.out.println(response);
		
	}

}
